{
  "manifest_version": 3,
  "name": "Re:invent Portal Extension",
  "version": "0.0.1",
  "description": "Making the re:invent portal better",
  "action": {
    "default_title": "Re:invent Portal Extension"
  },
  "permissions": [
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "run_at": "document_idle",
      "js": [
        "contentScript.js"
      ]
    }
  ]
}
